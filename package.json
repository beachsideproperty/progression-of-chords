    "@mui/material": "^5.11.12",
    "@mui/x-date-pickers": "^6.0.1",
    "@reduxjs/toolkit": "^1.9.3",
    "bcrypt": "^5.0.0",
    "bootstrap": "^5.2.3",
    "compression": "^1.7.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "history": "^4.9.0",
    "howler": "^2.2.3",
    "jsonwebtoken": "^8.5.1",
